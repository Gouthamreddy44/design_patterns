@startuml
package com.iluwatar.identityfield {
  class App {
    + App()
    + main(args : String[]) {static}
  }
  class Person {
    - id : long
    - name : string
    + Person(id : long,  name:string)
    + getName():string
    + setName(name:string)
    + getId() : long
    + setId(id:long)
  }
}
@enduml