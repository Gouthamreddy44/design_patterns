<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.iluwater.coarsegrainedlock.MybatisMapper">
    <insert id="insertCustomer">insert into Customer(customer_id,lastname,firstname,items_ordered) values (#{customerID},#{lastName},#{firstName},#{itemsOrdered})</insert>
    <insert id="insertAddress">insert into Address(customer_id,city,state) values (#{customerID},#{city},#{state})</insert>
</mapper>