package star_pattern;

public class HollowSquareWithDiagonal {
	public static void main(String[] args) {
		for (int i = 1; i <=10; i++) {
			for (int j = 1; j <=10; j++) {
				if(i==1 || j==1 || i==j|| i==10|| j==10 || i==11-j||j==11-i) {
					System.out.print("* ");
				}
				else {
					System.out.print("  ");
				}
			}
			System.out.println();
		}
	}

}

//i 9 8
//j 2 3
